</rdf:Description>

<rdf:Description rdf:nodeID="genid1">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<owl:unionOf rdf:nodeID="genid3"/>
</rdf:Description>

